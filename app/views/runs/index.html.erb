<h1>Runs</h1>

<% @runs.each do |run| %>
    <h2>Run <%= run.id %>
    <h4>Date: <%= run.date %></h4>
    <h4>Distance (miles): <%= run.distance %></h4>
    <h4>Duration (hours): <%= run.duration %></h4>
    <a href="/runs/<%=run.id%>/edit">Edit</a><br>
    <br>
<% end %>
